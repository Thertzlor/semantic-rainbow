{
   "name": "semantic-rainbow",
   "displayName": "Semantic Rainbow",
   "description": "A maximalist color theme for semantic highlighting featuring bold colors with subtle distinctions.",
   "repository": {
      "url": "https://github.com/Thertzlor/semantic-rainbow"
   },
   "publisher": "thertzlor",
   "author": {
      "name": "Thertzlor"
   },
   "icon": "assets/SR_icon128.png",
   "version": "1.0.0",
   "engines": {
      "vscode": "^1.60.0"
   },
   "categories": [
      "Themes"
   ],
   "keywords": [
      "semantic highlighting",
      "dark",
      "syntax highlighting",
      "maximalist",
      "colorful"
   ],
   "files": [
      "themes"
   ],
   "contributes": {
      "themes": [
         {
            "id": "semantic-rainbow",
            "label": "Semantic Rainbow",
            "uiTheme": "vs-dark",
            "path": "./themes/semantic-rainbow-color-theme.json"
         }
      ],
      "semanticTokenScopes": [
         {
            "scopes": {
               "parameter": [
                  "variable.parameter.function",
                  "variable.parameter.function-call",
                  "meta.function.parameter",
                  "meta.function.parameters"
               ],
               "function": [
                  "entity.name.function",
                  "meta.function-call"
               ],
               "variable": [
                  "variable.other.readwrite",
                  "variable.other",
                  "variable",
                  "support.variable",
                  "markup.heading",
                  "entity.name.section"
               ],
               "variable.readonly": [
                  "entity.name.tag"
               ],
               "type": [
                  "entity.name.type.alias",
                  "support.type"
               ],
               "namespace": [
                  "meta.symbol.namespace"
               ],
               "function.defaultLibrary": [
                  "support.function.builtin",
                  "support.function.misc",
                  "support.function"
               ],
               "property": [
                  "variable.other.property",
                  "support.variable.property",
                  "variable.other.object.property",
                  "meta.attribute",
                  "support.type.property-name",
                  "variable.object.property",
                  "meta.flow-mapping.yaml",
                  "support.type.property-name.css",
                  "meta.property-name",
                  "entity.other.attribute-name"
               ],
               "class": [
                  "entity.name.type.class",
                  "entity.other.inherited-class",
                  "entity.other.attribute-name.class"
               ],
               "method": [
                  "meta.directive.vue",
                  "entity.other.attribute-name.pseudo-class"
               ],
               "selfParameter": [
                  "variable.language.this",
                  "variable.language.self",
                  "variable.language.special.self"
               ],
               "variable.defaultLibrary.readonly": [
                  "const"
               ]
            }
         }
      ]
   },
   "dependencies": {
      "@types/node": "^16.9.1",
      "tinycolor2": "^1.4.2"
   }
}